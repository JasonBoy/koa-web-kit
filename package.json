{
  "name": "koa-web-kit",
  "version": "2.3.1",
  "description": "A modern, production-ready, and full-stack node web framework",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env ENABLE_HMR=true node app.js",
    "start": "node app.js",
    "deploy": "./deploy.sh",
    "build": "webpack --progress --hide-modules --config config/webpack.config.dev.js && echo '\n===Build for Development Done!==='",
    "watch": "webpack --watch --progress --hide-modules --config config/webpack.config.dev.js",
    "node": "webpack --progress --config config/webpack.config.node.js",
    "prod": "cross-env NODE_ENV=production webpack -p --progress --hide-modules --config config/webpack.config.prod.js && echo '\n===Build for Production Done!==='",
    "prod:analyze": "cross-env NODE_ENV=production BUNDLE_ANALYZER=true webpack -p --progress --hide-modules --config config/webpack.config.prod.js",
    "es6-module-to-commonjs": "babel test/es6/modules -d build/test/es6/modules --plugins transform-es2015-modules-commonjs",
    "test": "./node_modules/.bin/_mocha ./test/mocha",
    "precommit": "lint-staged",
    "build:wd": "webpack-dashboard -- npm run build",
    "watch:wd": "webpack-dashboard -- npm run watch",
    "dev:wd": "webpack-dashboard -- npm run dev"
  },
  "engines": {
    "node": ">= 8"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonBoy/koa-web-kit.git"
  },
  "keywords": [
    "koa",
    "es6",
    "webpack",
    "react",
    "bootstrap",
    "fullstack",
    "framework"
  ],
  "author": "jasonlikenfs@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JasonBoy/koa-web-kit/issues"
  },
  "homepage": "https://github.com/JasonBoy/koa-web-kit#readme",
  "readme": "README.md",
  "dependencies": {
    "bluebird": "3.5.1",
    "bootstrap": "4.0.0",
    "consolidate": "0.15.0",
    "get-port": "3.2.0",
    "koa": "2.4.1",
    "koa-body": "2.5.0",
    "koa-compress": "2.0.0",
    "koa-convert": "1.2.0",
    "koa-history-api-fallback": "0.1.3",
    "koa-morgan": "1.0.1",
    "koa-mount": "3.0.0",
    "koa-router": "7.4.0",
    "koa-session": "5.8.1",
    "koa-static": "4.0.2",
    "koa-views": "6.1.3",
    "lodash": "4.17.5",
    "lodash.isempty": "4.4.0",
    "moment": "2.20.1",
    "nunjucks": "3.0.1",
    "pm2": "2.10.1",
    "prop-types": "15.6.0",
    "qs": "6.5.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "request": "2.83.0",
    "request-debug": "0.2.0",
    "url-parse": "1.2.0",
    "whatwg-fetch": "2.0.3",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "autoprefixer": "7.2.6",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-minify-webpack-plugin": "0.3.0",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "chai": "4.1.2",
    "clean-webpack-plugin": "0.1.18",
    "copy-webpack-plugin": "4.4.1",
    "cross-env": "5.1.3",
    "css-loader": "0.28.9",
    "del": "3.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "html-loader": "0.5.5",
    "html-webpack-inline-chunk-plugin": "1.1.1",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "koa-webpack": "2.0.3",
    "lint-staged": "6.1.0",
    "mocha": "5.0.0",
    "moment-locales-webpack-plugin": "1.0.4",
    "node-sass": "4.7.2",
    "postcss-loader": "2.1.0",
    "prettier": "1.10.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.20.1",
    "uglifyjs-webpack-plugin": "1.2.2",
    "url-loader": "0.6.2",
    "webpack": "3.11.0",
    "webpack-bundle-analyzer": "2.10.1",
    "webpack-dashboard": "1.1.1",
    "webpack-manifest-plugin": "1.3.2",
    "webpack-merge": "4.1.1",
    "webpack-node-externals": "1.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
