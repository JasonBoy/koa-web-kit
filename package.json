  },
  "browserslist": [
    "> 1%",
    "not ie 11"
  ],
  "nodemonConfig": {
    "watch": [
      "api/",
      "config/",
      "build/node",
      "routes/",
      "utils/",
      "services/",
      "app-config.js",
      "server.js",
      "app.js"
    ]
  }
}
